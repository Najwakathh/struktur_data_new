    "def ls(arr, target):\n",
    "    for i in range(len(arr)):\n",
    "        if arr[i] == target :\n",
    "            return i\n",
    "    return -1\n",
    "\n",
    "data = [1,3,7,5]\n",
    "target = 5\n",
    "result = ls(data, target)\n",
    "if result == -1:\n",
    "    print(\"not found\")\n",
    "else:\n",
    "    print(f\"found di index ke - : {result}\")"
   ]
